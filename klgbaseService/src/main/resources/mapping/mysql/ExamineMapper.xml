            </if>
            <if test="jyTypeCode != null">
                jy_type_code,
            </if>
            <if test="cjbw != null">
                cjbw,
            </if>
            <if test="jgjldwCode != null">
                jgjldw_code,
            </if>
            <if test="jgzcbsCode != null">
                jgzcbs_code,
            </if>
            <if test="startCkzNan != null">
                start_ckz_nan,
            </if>
            <if test="endCkzNan != null">
                end_ckz_nan,
            </if>
            <if test="startCkzNv != null">
                start_ckz_nv,
            </if>
            <if test="endCkzNv != null">
                end_ckz_nv,
            </if>
            <if test="startCkzEt != null">
                start_ckz_et,
